wget -O neo4j-community-5.11.0-unix https://neo4j.com/artifact.php?name=neo4j-community-5.11.0-unix.tar.gz
tar xfz neo4j-community-5.11.0-unix

./neo4j-community-5.11.0/bin/neo4j-admin dbms set-initial-password password
./neo4j-community-5.11.0/bin/neo4j start
./neo4j-community-5.11.0/bin/cypher-shell -u neo4j -p password